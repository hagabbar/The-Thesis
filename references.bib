@ARTICLE{10.3389/fspas.2020.00028,
  
AUTHOR={Schmidt, Patricia},   
	 
TITLE={Gravitational Waves From Binary Black Hole Mergers: Modeling and Observations},      
	
JOURNAL={Frontiers in Astronomy and Space Sciences},      
	
VOLUME={7},      

PAGES={28},     
	
YEAR={2020},      
	  
URL={https://www.frontiersin.org/article/10.3389/fspas.2020.00028},       
	
